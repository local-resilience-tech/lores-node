{
  "editor.formatOnSave": true,
  "html.format.wrapLineLength": 140,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true,
    "**/.ruby-lsp": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "backend/target": true,
    "frontend/dist": true,
    "node_modules": true,
    "backend/.sqlx": true,
    "backend-rocket/target": true,
    "frontend/node_modules": true
  },
  "explorerExclude.backup": {},
  "rust-analyzer.cargo.sysroot": "discover",
  "cSpell.words": [
    "Aquadoggo",
    "iroh",
    "Liftoff",
    "Scrib"
  ],
  "ansible.python.interpreterPath": "/bin/python3",
  "editor.wordWrapColumn": 140,
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": false // Optional: Disable automatic indentation detection
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": false // Optional: Disable automatic indentation detection
  },
}