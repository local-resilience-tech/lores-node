{
  "editor.formatOnSave": true,
  "html.format.wrapLineLength": 140,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "backend/target": true,
    "frontend/dist": true,
    "node_modules": true
  },
  "explorerExclude.backup": {},
  "rust-analyzer.cargo.sysroot": "discover",
  "cSpell.words": ["Aquadoggo", "iroh", "Liftoff", "Scrib"],
  "ansible.python.interpreterPath": "/bin/python3",
  "editor.wordWrapColumn": 140
}
